<h2>edit me!</h2>
<form class="col-md-6" method="POST" action="/auroras/<%= aurora.id %>">
  <input type="hidden" name="_method" value="PUT">
  <div>
    <label for="country">Place where the photo was taken</label>
    <input id="pac-input" name="country" class="card form-control" type="text" value="<%= aurora.country %>" placeholder="Please input location">
    <div id="map"></div>
  </div>
  <button class="btn btn-primary">Save changes</button>
</form>
