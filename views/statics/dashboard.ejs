<h2>DASHBOARD</h2>
<div class="row">
  <div class="col-md-6">
    <h3>FORECAST</h3>
  </div>
  <div class="col-md-6">
    <h3>PICS HERE</h3>

    <div class="row">
      <% auroras.forEach((aurora) => { %>
          <div class="col-md-6 picture-box">
          <a href="/auroras/<%= aurora.id %>">
          <div class="card" style="background-image: linear-gradient(rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0.2)),url(<%= aurora.imageSRC %>)">
            <h4><%= aurora.country %></h4>
            </div>
            </a>
          </div>
        <% }); %>
      </div>

  </div>

</div>
